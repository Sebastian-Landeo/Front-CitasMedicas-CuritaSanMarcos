<?xml version="1.0" encoding="utf-8"?>
<FrameLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.LoginActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="34dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/extra_bold"
        android:text="Bienvenido"
        android:textAlignment="center"
        android:textColor="#2260FF"
        android:textColorLink="#2260FF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="325dp"
        android:layout_height="54dp"
        android:fontFamily="@font/light"
        android:text="Dedicamos a brindar atenciones de calidad con la noble misión de  cuidar la salud integral de nuestros pacientes desde hace 35 años."
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="14sp"
        android:textStyle="normal"
        android:layout_marginStart="45dp"
        android:layout_marginTop="250dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:layout_width="329dp"
        android:layout_height="42dp"
        app:cardCornerRadius="16dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="370dp"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/Password"
            android:layout_width="329dp"
            android:layout_height="match_parent"
            android:background="#ECF1FF"
            android:ems="10"
            android:fontFamily="@font/light"
            android:hint="Correo electrónico"
            android:inputType="textEmailSubject"
            android:padding="13dp"
            android:textColor="#809CFF"
            android:textColorHint="#809CFF"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/light"
        android:text="¿No tienes una cuenta creada?"
        android:layout_marginStart="90dp"
        android:layout_marginTop="610dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonRegistro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:fontFamily="@font/semi_bold"
        android:letterSpacing="-0.01"
        android:text="Registrate"
        android:textAllCaps="false"
        android:textColor="#2260FF"
        android:textSize="14sp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="595dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="207dp"
        android:layout_height="60dp"
        android:backgroundTint="#2260FF"
        android:fontFamily="@font/semi_bold"
        android:text="Iniciar Sesión"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColorLink="#2260FF"
        android:textSize="24dp"
        android:textStyle="normal"
        android:layout_marginStart="100dp"
        android:layout_marginTop="550dp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:text="Correo electrónico"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginStart="45dp"
        android:layout_marginTop="320dp"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:layout_width="329dp"
        android:layout_height="45dp"
        app:cardCornerRadius="16dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="480dp"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/PasswordInicio"
            android:layout_width="329dp"
            android:layout_height="match_parent"
            android:background="#ECF1FF"
            android:ems="10"
            android:fontFamily="@font/light"
            android:hint="@string/contrase_a"
            android:inputType="textPassword"
            android:padding="13dp"
            android:textColor="#809CFF"
            android:textColorHint="#809CFF"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="@string/logo"
        android:src="@drawable/group_93"
        android:layout_marginStart="155dp"
        android:layout_marginTop="100dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semi_bold"
        android:text="Contraseña"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginStart="45dp"
        android:layout_marginTop="430dp"
        tools:ignore="MissingConstraints" />


</FrameLayout >