<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.RegisterSuccessfulActivity">

    <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="#2260FF"
    tools:ignore="UselessParent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="105dp"
            android:layout_marginTop="120dp"
            app:srcCompat="@drawable/vector" />

        <TextView
            android:id="@+id/text_nombre"
            android:layout_width="268dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="330dp"
            android:fontFamily="@font/medium"
            android:gravity="center_horizontal"
            android:text="Felicidades"
            android:textColor="#FFFFFF"
            android:textSize="48sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/text_app"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="130dp"
            android:layout_marginTop="400dp"
            android:fontFamily="@font/medium"
            android:gravity="center_horizontal"
            android:text="Te registraste exitosamente"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/text_app_2"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="130dp"
            android:layout_marginTop="520dp"
            android:fontFamily="@font/light"
            android:gravity="center_horizontal"
            android:text="Vamos a por tu cita"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button"
            android:layout_width="207dp"
            android:layout_height="60dp"
            android:backgroundTint="@color/white"
            android:fontFamily="@font/semi_bold"
            android:text="Empezar"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            android:textColor="#2260FF"
            android:textSize="24dp"
            android:textStyle="normal"
            android:layout_marginStart="100dp"
            android:layout_marginTop="570dp"
            app:cornerRadius="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <FrameLayout
            android:layout_width="330dp"
            android:layout_height="200dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="480dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/border_frame"
            android:padding="15dp">
        </FrameLayout>

    </FrameLayout>

</FrameLayout>